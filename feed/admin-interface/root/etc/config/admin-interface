	option onboarding_needed 'true'
	option vpn_status 'true'
	option vpn_from_account 'false'
	option separate_ipsec_credentials 'true'
	option support_url 'http://help.test.com'
	option support_email 'support@test.com'
	option openvpn_credentials_url 'http://openvpn.creds'
	option ipsec_credentials_url 'http://ipsec.creds'

