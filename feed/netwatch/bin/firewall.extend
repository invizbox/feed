device_ip=$(uci get network.lan.ipaddr)
iptables -t nat -I POSTROUTING -o eth0.2 -m comment --comment "!fw3" -j MASQUERADE
iptables -t nat -I OUTPUT -d 10.192.0.0/16 -p tcp --syn -m comment --comment "!fw3" -j DNAT --to-destination 172.31.1.1:9040
iptables -I INPUT -i br-lan -d 172.31.1.1 -p tcp --dport 9040 -m comment --comment "!fw3" -j REJECT --reject-with tcp-reset
iptables -I FORWARD -i eth0.2 -o br-lan -m conntrack --ctstate RELATED,ESTABLISHED -m comment --comment "!fw3" -j ACCEPT
iptables -I FORWARD -i br-lan -o eth0.2 -m conntrack --ctstate NEW,RELATED,ESTABLISHED -m comment --comment "!fw3" -j ACCEPT
iptables -I FORWARD -d ${device_ip}/32 -i br-lan -o br-lan -m conntrack --ctstate NEW,RELATED,ESTABLISHED -m comment --comment "!fw3" -j ACCEPT
iptables -D FORWARD -m conntrack --ctstate RELATED,ESTABLISHED -m comment --comment "!fw3" -j ACCEPT
